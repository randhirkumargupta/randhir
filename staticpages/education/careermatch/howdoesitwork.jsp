<script type="text/javascript">    
	var button;
	
	window.fbAsyncInit = function() {
		FB.init({ appId: '613580381995012', //change the appId to your appId
			status: true, 
			cookie: true,
			xfbml: true,
			oauth: true});

	   function updateButton(response) {
			button       =   document.getElementById('fb-auth');
			
			if (response.authResponse) {
				//user is already logged in and connected
				FB.api('/me', function(info) {
					login(response, info);
				});
				
				button.onclick = function() {
					FB.logout(function(response) {
					
					document.getElementById('userName').type		   = "";
					document.getElementById('email').type	 	   = "";
					document.getElementById('userName').value		   = "";
					document.getElementById('email').value		   = "";
					document.getElementById('sn_image').value	   = "";
					document.getElementById('sn_source').value	   = "";
					logout(response);
					});
				};
			} else {
				//user is not connected to your app or logged out
				button.innerHTML = '<img src="http://itgd-mum-dev-static.s3.amazonaws.com/media/others/mediaintoday/microsites/learntoday/careermatch/login-with-facebook.jpg">';
				button.onclick = function() {
					FB.login(function(response) {
						if (response.authResponse) {
							FB.api('/me', function(info) {
								login(response, info);
							});	   
						} else {
							//user cancelled login or did not grant authorization
						
						}
					}, {scope:'email,user_birthday,status_update,publish_stream,user_about_me'});  	
				};
			}//end-else
		}//end updateButton
		
		// run once with current status and whenever the status changes
		FB.getLoginStatus(updateButton);
		FB.Event.subscribe('auth.statusChange', updateButton);	
	};
	(function() {
		var e = document.createElement('script'); e.async = true;
		e.src = document.location.protocol 
			+ '//connect.facebook.net/en_US/all.js';
		document.getElementById('fb-root').appendChild(e);
	}());
	
				
	function login(response, info){
		console.log(info);
		
		if (response.authResponse  && info.name !='' && info.name != undefined) {
			$('.popup').hide();
			document.getElementById('userName').value					=	info.name;
			document.getElementById('email').value						=	info.email;
			var hrefUrl = 	'http://indiatoday.intoday.in/education/careermatch/checkusername.jsp?userName='+info.name+'&email='+info.email+'&login_source=facebook';
			
			$(location).attr('href', hrefUrl);
			var accessToken												=	response.authResponse.accessToken;
			document.getElementById('logoutWithFb').style.display		= 	"block";
			document.getElementById('loginWithFb').style.display		= 	"none";
			document.getElementById('userName').type					= 	"hidden";
			document.getElementById('email').type						= 	"hidden";
			document.getElementById("facebook").style.backgroundImage	=	"url(http://itgd-mum-dev-static.s3.amazonaws.com/media/others/mediaintoday/microsites/education/logout-fb.png)";
								
			document.getElementById('sn_image').value					=	'https://graph.facebook.com/' + info.id + '/picture';
			document.getElementById('sn_source').value					=	'facebook';
		}
	}
				
	function logout(response){
		document.getElementById("facebook").style.backgroundImage		=	"backgroundurl(http://itgd-mum-dev-static.s3.amazonaws.com/media/others/mediaintoday/microsites/education/sign-fb.png); background-repeat: no-repeat;";
		var hrefUrl = 	'http://indiatoday.intoday.in/education/careermatch/index.jsp';
		
			$(location).attr('href', hrefUrl);
		document.getElementById('userName').value						= 	"";
		document.getElementById('email').value							=	"";
		document.getElementById('userName').type						=	"text";
		document.getElementById('email').type							=	"text";
		document.getElementById('sn_image').value						=	"";
		document.getElementById('sn_source').value						=	"";
	}	
	
		//stream publish method
	function streamPublish(name, description, hrefTitle, hrefLink, userPrompt){
	
		FB.ui(
		{				
			method: 'stream.publish',
			message: '',
			attachment: {
				name: name,
				caption: '',
				description: (description),
				href: hrefLink
			},
			action_links: [
				{ text: hrefTitle, href: hrefLink }
			],
			user_prompt_message: userPrompt
		},
		function(response) {
		
		});

	}
	function showStream(){
		FB.api('/me', function(response) {
			//message = document.getElementById('message').value;
			message ="";
			streamPublish(location.href, message,'hrefTitle',location.href, "Share IndiaToday");
		});
	}

	function share(){
		
		var share = {
			method: 'stream.share',
			u: 'http://indiatoday.intoday.in/'
		};

		FB.ui(share, function(response) {					
			console.log(response); 
		});
	}			
</script>
