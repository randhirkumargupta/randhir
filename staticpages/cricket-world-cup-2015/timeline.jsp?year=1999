<script type="text/javascript">
(function($){
		$(window).load(function(){
			$("#amazing-facts").mCustomScrollbar({});
		});
		
		$('ul.slider_thumb li').click(function(){
			$('ul.slider_thumb li a').removeClass('hover')
			$(this).children().addClass('hover')
			for(var i=1;i<=5;i++)
			{$('#sliderimg'+i).hide()}
			$('#sliderim'+$(this).children('a').attr('rel')).show();
			})
		return false;
		
	})(jQuery);
</script>




