        <script>
	$(document).ready(function(){
	         
		 	$('.belt-outer').children('.catlist:first').addClass('active').css({'background-color':'','display': 'block'});
			$('.belt-outer').children('.heading:first').addClass('active').css({'display': 'block'});
			  $('.btm_belt ul li').eq(0).addClass('activethumb');
			  var boxnew_counter = 1;
			var box1_len = $('.catlist.active span').length;


	          $('.btm_belt ul li').click(function(){ 
			
			  $('.slide-next').show();
				$('.slide-back').hide();
			 box1_counter = 1;
			 $('.catlist').css('left', '0');
			  
			 
			  $('.btm_belt ul li').removeClass('activethumb');
			     $(this).addClass('activethumb'); 
               var index = $('.btm_belt ul li').index($(this));
			   //alert(index);
			  $('.catlist').removeClass('activethumb').css({'display':'none'});			   
			  $('.box'+index).addClass('activethumb').css({'background-color':'','display': 'block'});
			  
			  $('.heading').removeClass('activethumb').css({'display':'none'});
			  $('.slider_heading' + index).addClass('activethumb').css({'display': 'block'});;
			  
			 	  box1_len = $('.catlist.active span').length;
					var boxnew_widt = $('.catlist.active span').width();
					var boxnew_widt = boxnew_widt * box1_len;
					$('.catlist.active').css('width', boxnew_widt);
	});				
	

               
			  
		       var box1_widt = $('.catlist.active span').width();
		       var box1_widt = box1_widt * box1_len;
			   $('.catlist.active').css('width', box1_widt);		
		       var box1_counter = 1;	
		$('.slide-next').click(function(){
		$('.slide-back').show();
			if (box1_counter < box1_len)
				{
				$('.catlist.active').animate({
					left :'-=660',
				})
			box1_counter += 1;
			
		}else
		{
			$('.slide-next').hide();
		}
		});
		
		
		$('.slide-back').click(function(){
		$('.slide-next').show();
			if (box1_counter >= 2)
				{
				$('.catlist.active').animate({
					left :'+=660',
				})
			box1_counter -= 1;
		}else
		{
			$('.slide-back').hide();
		}
		});
		
		
		
		var btm_len = $('.btm_belt ul li').length;
		var btm_width = $('.btm_belt ul li').width();

		btm_width = (btm_width+20)* btm_len;
		$('.btm_belt ul').css('width', btm_width);
		
		var btm_counter = 1;
		$('.btm_right').click(function(){
		$('.btm_left').show();
			if (btm_counter < btm_len-2)
				{
				$('.btm_belt ul').animate({
					left :'-=164',
				})
			btm_counter += 1;
		}else
		{
			$('.btm_right').hide();
		}
		});
		
		
		$('.btm_left').click(function(){
		$('.btm_right').show();
			if (btm_counter >= 2)
				{
				$('.btm_belt ul').animate({
					left :'+=164',
				})
			btm_counter -= 1;
		}else
		{
			$('.btm_left').hide();
		}
		});
		
		var value = 0;
		$('.btm_belt ul li').each(function(){
			value += 1;
			$(this).addClass('current_'+ value);
		})
	
	});
</script>
        <script>
/*main slider*/
$(document).ready(function(e) {
	var lenr = $(".right-slider ul li").length;
	var widthr = $(".right-slider ul li").width();
	widthr = widthr * lenr;
	$(".right-slider ul").css('width', widthr);



	var li1height = $('#_1').height();
	$('.belt').height(li1height);
	
	var len = $(".belt li").length;
	var tWidth = $(".belt li").width();	
	var fWidth = tWidth * len;
	$(".belt").css('width', fWidth)
	 var counter = 1;
	 var liheight;
	 
	 //NEXT
	 $('.next').click(function(){
		
		 if(counter < len){
		   $('.belt').animate({'left' :'-=608'})
		   $(".right-slider ul").animate({'left' :'-=300'});
		   $('.prev').fadeIn();
		  
		   counter +=1;
		   var liheight = $('#_'+counter).height();
		   $('.belt').height(liheight);
		   
		 }
		 else{
			 $('.next').fadeOut();
			 
		 }
_gaq.push(['_trackEvent', "topemployer", "next",1]);
	 });
	 
	 //PREV
	 $('.prev').click(function(){
		 if(counter == 1){
		    $('.prev').fadeOut();
			 
		 }else{
			 $('.belt').animate({'left' :'+=608'});
			 $(".right-slider ul").animate({'left' :'+=300'});
			 $('.next').fadeIn();
			  
		     counter -=1;
			 var liheight = $('#_'+counter).height();
		  	 $('.belt').height(liheight);
			 
		 }
_gaq.push(['_trackEvent', "topemployer", "previous",1]);
	 });
	 
	 //NAV
	 $('.nav-area li a').click(function(){
		 $('.nav-area li a').removeClass('active')
		 $(this).addClass('active');
	 });
	 
	 var vidcounter = 1
	 var vidlen = $('.video-belt').children('div').length;
	
	 var vidheight = $('.video-belt').children('div').height();
		vidheight = vidheight *vidlen;
	$('.video-belt').css('height', vidheight);
	
	$('.seemore').click(function(){
		$('.video-belt').animate({'top' : '-=115'});
		
		vidcounter += 1;
		if (vidcounter >= vidlen - 6)
		{
			$('.seemore').addClass('seemoredown');
			$('.seemore').removeClass('seemore');
			$('.down-arrow').addClass('up-arrow');
			$('.down-arrow').removeClass('down-arrow');
			
		}
	});
	 
});
/*main slider end*/
</script>


