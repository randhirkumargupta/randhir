<script type="text/javascript">
var maxCounter=959-1;
function showImg(counter, startNo, endNo) {
	//alert(counter+"----"+startNo+"----"+endNo);
	var htmlimg = $('#img'+counter+' > img').attr('bigsrc');
	$('.popup_box').show(10);
	$('#wrappercont').css({'opacity' : '0.5', 'background-color':'#000' });
	if(counter<endNo-1){
	//pageTracker._trackPageview(''+counter+'#BN');
		_gaq.push(['_trackPageview', 'http://indiatoday.intoday.in/understand/index.jsp?page=80&imgId='+counter+'#BN']);
		var next = '<a href="javascript:void(0);" onclick="showImg('+(parseInt(counter)+1)+', '+startNo+', '+endNo+');" class="nextarrow">Next</a>';
	} else {
		var next = '';	
	}
	if(counter>startNo){
	//pageTracker._trackPageview(''+counter+'#BN');
		_gaq.push(['_trackPageview', 'http://indiatoday.intoday.in/understand/index.jsp?page=80&imgId='+counter+'#BN']);
	 	var prev = '<a href="javascript:void(0);" onclick="showImg('+(parseInt(counter)-1)+', '+startNo+', '+endNo+');" class="prevarrow">Previous</a>';
	} else {
		var prev = '';
	}
	//alert(""+htmlimg);	
	
	var socialshare ='<div class="frm-share rght"><div class="socialcont"><div class="socialdiv"><a href="http://www.facebook.com/sharer.php?u='+htmlimg+'" title="Facebook" target="_new" rel="nofollow"><span class="imgs fb lft"></span></a><a href="http://twitter.com/?status='+htmlimg+'%20via%20%40indiatoday" title="Twitter" target="_new" rel="nofollow"><span class="imgs tw lft"></span></a></div></div></div>';
	$('#fun-container').css("display","block");
	$('#popimg').html(socialshare+next+prev+'<img src="'+htmlimg+'">');
	
}
</script>


