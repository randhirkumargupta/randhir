$(document).ready(function() {
if (/(iPhone|iPod|iPad)/i.test(navigator.userAgent)) {}else if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)) {}else {
    storyEssentials();
	}
});
function storyEssentials() {
    $('.right-story-container p').not('p-share').bind('mouseup', function(e) {     
        $('.twoverlay').hide(10);
        var selection;
        if (window.getSelection) {
            selection = window.getSelection();
        } else if (document.selection) {
            selection = document.selection.createRange();
        }
        try {
            if (selection.toString() !== '' && selection.toString() !== ' ' && !selection.toString().match('\n')) {
                $('.twoverlay').css({
                    top: e.pageY + 'px',
                    left: e.pageX + 'px'
                });
                $('.twoverlay').show(10);
                var texttosend = selection.toString();
                $('.twoverlay img').click(function() {
                    var pageurl = $('link[rel=canonical]').attr('href');
                    $('.twoverlay').hide(10);
                    if ($(this).attr('id').match('twsend')) {
                        pageurl = "http://indiatoday.intoday.in/story/?sId=114431";
                        userhandle = " @indiatoday";
                        var fulluri = userhandle + ' ' + pageurl;                      
                        var remaining = 140 - parseInt(fulluri.length);
                        if (remaining > 5) {
                            texttosend = '"' + texttosend.substring(0, (parseInt(remaining) - 2)).trim() + '"';
                        } else {
                            texttosend = '"' + texttosend.substring(0, 10).trim() + '"';
                        }
                        fulluri = escape(texttosend + fulluri);
                        var openuri = "https://twitter.com/intent/tweet?text=";
                        window.open(openuri + fulluri, 'twwindow', "width=550, height=420").focus();
                    }
                });
            }
        } catch (er) {}
    });
}
