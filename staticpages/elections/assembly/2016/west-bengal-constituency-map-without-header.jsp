		combined.push([pArray[i], parseInt(sArray[i])]);
		totalASeats = parseInt(totalASeats) + parseInt(sArray[i]);
		grandtotal+=parseInt(sArray[i]);
	}
	tultip = {pointFormat: '{series.name}: <b>{point.y}</b>'};
