	  }
      $("#consTable").html("<table border = '1' cellspacing='0' cellpadding='0' style='width: 220px;'>"+tr+"</table>");
$('#dynamic_select option').removeAttr('selected');
$('#constiuency_'+consID).attr('selected','selected');
    },
