      var json_obj = mapData;
 
          
    setTimeout(function(){
       var content ='';
       showData.empty();
          if(json_obj[district]!=""){
      for(var i in json_obj[district])    
      {

      
        content = '<div class="candidate_row c'+json_obj[district][i].win_loss+'"><div class="left_part_cn"><img src="https://electionresults.intoday.in/elections/2017/assets/images/image.gif" class="candidate_image"><span class="candidate_desc"><h3>'+json_obj[district][i].candidate+'</h3><h4>'+json_obj[district][i].party+'</h4></span></div><div class="right_part_cn"><span class="win_lose '+json_obj[district][i].win_loss+'">'+json_obj[district][i].win_loss+'</span><span class="share_icon_t"><a href="#" title=""></a></span></div></div>';
          showData.append(content);  
    
      }  
      }
      else
      {
       showData.html('No Data');    
       console.log("No Data");     
      }

      if(areaid != undefined){
      var mapObj = map.getObjectById(areaid);
      if(mapObj != undefined){
        map.clickMapObject(mapObj);
      }
     }  
    },500);
   

  }   
  else
  {
    showData.empty();
    var content = '';
        showData.append(content);
    console.log("No Data");
  }
}catch(e) {
