	function checkAds(adsId){
	var c =  $('#'+adsId).attr('data-zedoads');
	if (document.getElementById(adsId)!=undefined){
		if($('#'+adsId).height()==0 || $('#'+adsId).height()=='0'){
		ga('send','event','Adblock',c,'true'); //ga('send','_trackEvent','WriteAdblock','true');
		}	
