$(document).ready(function() {
	$("#tab2").hide();
	$("#tab3").hide();
	$("#taba").click(function(){
		$("#tab1").show().slideDown(300);
		$("#tab2").hide().slideUp(300);
		$("#tab3").hide().slideUp(300);
		$("#taba").removeClass("grey");
		$("#tabbb").addClass("grey");
		$("#tabb").addClass("grey");
