		$("#tab2").show().slideDown(300);
		$("#tab1").hide().slideUp(300);
		$("#tab3").hide().slideUp(300);
		$("#tabbb").addClass("grey");
		$("#tabb").removeClass("grey");
		$("#taba").addClass("grey");
	});
	
	
	$("#tabbb").click(function(){
		$("#tab2").hide().slideUp(300);
		$("#tab1").hide().slideUp(300);
		$("#tab3").show().slideDown(300);
		$("#tabbb").removeClass("grey");
		$("#tabb").addClass("grey");
		$("#taba").addClass("grey");
	});
	
	
});
