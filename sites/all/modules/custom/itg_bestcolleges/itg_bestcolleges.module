/**
 * function for itg_bestcollege_image_slider().
 * @return type
 */
function itg_bestcollege_image_slider() {
  $url_get = explode('/', $_SERVER['REQUEST_URI']);
  $year = isset($url_get[2]) ? $url_get[2] : '';
  $story_data = array();
  $json = variable_get('bestcollege_get_video', '');
  $related_result = (array) json_decode($json);
  $image_slider = $related_result['image_slider'];
  if (!empty($year)) {
    if (is_object($related_result['image_slider']) && count($related_result['image_slider']) > 0) {
      $story_data = $related_result['image_slider']->{$year};
    }
    if (is_array($story_data) && count($story_data) > 0) {
		$query = db_select('node', 'n');
		$query->leftjoin('field_data_field_story_extra_large_image', 'sm', 'sm.entity_id = n.nid');
		$query->fields('n', array('nid', 'title'));
		$query->fields('sm', array('field_story_extra_large_image_fid'));
		$query->condition('n.type', 'story');
		$query->condition('n.nid', ($story_data), 'IN');
		return $results = $query->execute()->fetchAll(PDO::FETCH_ASSOC);
    }
  }
}

