<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:media="http://search.yahoo.com/mrss/" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <title>{{{web_site_name}}}| {{{section_category_name}}}</title>
        <description>{{{web_site_name}}}</description>
        <link><![CDATA[  {{{website_url}}} ]]></link>
        <lastBuildDate>{{{xml_creation_date}}}</lastBuildDate>
        <generator>FeedCreator 1.7.2</generator>
        <image>
            <url> {{{web_logo_url}}} </url>
            <title> {{{web_site_name}}} | {{{section_category_name}}} </title>
            <link> {{{category_url}}} </link>
            <description> {{{web_site_name}}} </description>
        </image>
        <item>
            <title> {{{node_title}}} </title>
            <link> {{{node_permalink}}} </link>
            <description>
                <a href="{{{mrss_node_permalink}}}"><img src="{{{field_story_extra_small_image}}}" align="left" hspace="2" height="82" width="107" alt="" border="0" /></a>{{{field_story_expert_description}}}<br clear="all"/>
            </description>
            <pubDate> {{{node_published_date}}} </pubDate>
            <media:content type="video/quicktime" expression="full" url="{{{mrss_node_permalink}}}">
                <media:category scheme="{{{mrss_node_permalink}}}">{{{section_category_name}}}</media:category>
                <media:text type="plain"> {{{field_story_expert_description}}} </media:text>
                <media:rating scheme="urn:simple"> nonadult </media:rating>
                <media:thumbnail width="107" height="82" url="{{{field_story_extra_small_image}}}"/>
            </media:content>
        </item>
    </channel>
</rss>