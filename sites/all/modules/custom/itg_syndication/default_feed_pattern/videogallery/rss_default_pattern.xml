<rss version="2.0" xmlns:media="http://search.yahoo.com/mrss/" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <title>{{{web_site_name}}}| {{{section_category_name}}}</title>
        <description>{{{web_site_name}}}</description>
        <link><![CDATA[  {{{website_url}}} ]]></link>
        <lastBuildDate>{{{xml_creation_date}}}</lastBuildDate>
        <generator>FeedCreator 1.7.2</generator>
        <image>
            <url> {{{web_logo_url}}} </url>
            <title> {{{web_site_name}}} | {{{section_category_name}}} </title>
            <link> {{{section_category_name}}} </link>
            <description> {{{web_site_name}}} </description>
        </image>
        <item>
            <title> {{{node_title}}} </title>
            <link> {{{node_permalink}}} </link>
            <description><![CDATA[  <a href="{{{mrss_node_permalink}}}"> <img align="left" hspace="2" height="82" width="107" alt="" border="0" src="{{{field_story_extra_large_image}}}"> </a> ]]> {{{field_story_expert_description}}} <![CDATA[ <br clear='all'> ]]> </description>
            <pubDate> {{{node_published_date}}} </pubDate>
        </item>
    </channel>
</rss>