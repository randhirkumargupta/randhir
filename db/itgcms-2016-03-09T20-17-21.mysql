INSERT INTO `workbench_moderation_states` VALUES ('archive','Archive',' For soft delete content','0'),('draft','Draft','Work in progress','-10'),('needs_modification','Needs Modification','Mark as Needs Modification assign to copy editor, section editor and editor','-7'),('needs_review','Submitted for Review','Ready for moderation','-9'),('published','Published','Make this version live','-5'),('rejected','Rejected','Rejected for copy editor, section editor and editor','-8'),('un_published','Un published','For un publish content','0');
