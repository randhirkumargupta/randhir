INSERT INTO `field_revision_field_recipe_kicker_headline` VALUES ('node','recipe','0','337','485','und','0','चिकन का नाम सुनते ही मुंह में आ गया न पानी तो क्‍यों न आज बनाई जाए चिकन की मसालेदार रेसिपी बटर चिकन मसाला. बाजार के मसाले सेहत को नुकसान पहुंचा सकते हैं इ‍सलिए घर पर ही चखें जायकेदार बटर चिकन मसाला का स्‍वाद.',null),('node','recipe','0','350','499','und','0','स्‍नैक्‍स में समोसा काफी पसंद किया जाता है. आइए इसकी रेसिपी में डालते हैं थोड़ा सा ट्विस्‍ट और बनाते हैं समोसा रोल की रेसिपी.',null),('node','recipe','0','358','507','und','0',' बर्तन में चावल डालकर अच्छी तरह मैश कर लें.',null);
